{
  "kind": "collectionType",
  "collectionName": "issues",
  "info": {
    "singularName": "issue",
    "pluralName": "issues",
    "displayName": "Issue",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "name": {
      "type": "string"
    },
    "publication_date": {
      "type": "date"
    },
    "description": {
      "type": "blocks"
    },
    "cover_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
